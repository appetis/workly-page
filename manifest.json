{
  "manifest_version": 2,
  "name": "Workly",
  "homepage_url": "http://localhost:8080/",
  "description": "A Vue Browser Extension",
  "default_locale": "en",
  "permissions": [
    "<all_urls>",
    "*://*/*"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_title": "__MSG_extName__",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "version": "0.1.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "key": ""
}